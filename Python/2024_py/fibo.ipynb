    "# fibomod.py\n",
    "def fibo(n):\n",
    "    if n<=0:\n",
    "        print(\"input should be a +ve integer\")\n",
    "    elif n==1:\n",
    "        return 0\n",
    "    elif n==2:\n",
    "        return 1\n",
    "    else:\n",
    "        return fibo(n-1)+fibo(n-2)"
